{
    "name": "mortenscheel/php-rusty",
    "description": "Rust Option and Result in PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Scheel\\Rusty\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Morten Scheel",
            "email": "morten@mortenscheel.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "phpstan/phpstan": "^1.11",
        "pestphp/pest": "^2.34",
        "phpstan/extension-installer": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
